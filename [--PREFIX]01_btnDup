//[--PREFIX]01_btnDup //method_generator [--DATE]//[--TABLE] 複製C_LONGINT($[--PREFIX]_id;$new_[--PREFIX]_id)$[--PREFIX]_id:=0C_LONGINT($selCnt)C_LONGINT($dlg_ok)//システム変数OKを保持$selCnt:=JCL_lst_SelectedCount (->v[--PREFIX]01_lst[--PREFIX])If ($selCnt>=1)//リストボックスに選択行があれば$[--PREFIX]_id:=JCL_lst_Selected_Long (->v[--PREFIX]01_lst[--PREFIX];->v[--PREFIX]01_lst[--PREFIX]_ID)$dlg_ok:=[--PREFIX]02_Input_Dup ($[--PREFIX]_id;->$new_[--PREFIX]_id)//入力フォームを開くIf ($dlg_ok=1)[--PREFIX]01_lst[--PREFIX]_Make //テーブルからリロード//変更行を選択状態にするJCL_lst_SetSelect_byLong (->v[--PREFIX]01_lst[--PREFIX];->v[--PREFIX]01_lst[--PREFIX]_ID;$new_[--PREFIX]_id)[--PREFIX]01_SetControlsValues End if End if 