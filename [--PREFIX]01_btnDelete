//[--PREFIX]01_btnDelete//method_generator v20141003 [--DATE]//[--TABLE] リストボックスの選択行を複数削除 確認ダイアログを表示C_LONGINT($[--PREFIX]_id)$[--PREFIX]_id:=0C_LONGINT($selCnt)C_LONGINT($dlg_ok)//システム変数OKを保持$selCnt:=Jiro_lst_SelectedCount (->v[--PREFIX]01_lst[--PREFIX])If ($selCnt>=1)//リストボックスに選択行があれば//アラートで確認C_TEXT($title;$msg;$okStr;$cancelStr)$title:="[--TABLE] 削除"$msg:=String($selCnt)+"件の [--TABLE] を削除します。よろしいですか？"$okStr:="削除"$cancelStr:="キャンセル"$dlg_ok:=Jiro_dlg_YesNo ($title;$msg;$okStr;$cancelStr)If ($dlg_ok=1)[--PREFIX]01_DeleteSelectedItems //複数削除[--PREFIX]01_lst[--PREFIX]_Make //テーブルからリロード//選択状態をなしにするJiro_lst_Deselect (->v[--PREFIX]01_lst[--PREFIX])[--PREFIX]01_SetControlsValues End if End if 